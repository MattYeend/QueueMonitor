{
    "name": "mattyeend/prevent-destructive-commands",
    "description": "A Laravel package to stop destructive commands.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "Laravel",
        "Queue",
        "Monitor",
        "Monitoring",
        "Queue-Monitor",
        "Queue-Monitoring",
        "Package"
    ],
    "authors": [
        {
            "name": "Matt Yeend",
            "email": "mail@mattyeend.co.uk"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "laravel/framework": "^9.0 | ^10.0 | ^11.0",
        "illuminate/support": "^9.0 | ^10.0 | ^11.0"
    },
    "autoload": {
        "psr-4": {
            "Mattyeend\\QueueMonitoring\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MattYeend\\QueueMonitoring\\QueueMonitoringServiceProvider"
            ]
        }
    }
}
